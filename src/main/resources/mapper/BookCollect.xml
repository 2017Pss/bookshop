<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.daniel.dao.BookCollectDAO">

        <insert id="add" useGeneratedKeys="true" keyProperty="id" parameterType="BookCollect" >
            insert into book_collect
                ( book_id,student_id)
            values
                (#{bookId},#{studentId})
        </insert>

        <select id="getBookByStudentId" parameterType="String" resultType="BookCollect" >
            select * from book_collect where student_id = #{id}
        </select>

</mapper>